
{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.1.0",
    "generators": {
      "v1": {
        "generatorName": "typescript-axios",
        "output": "#{cwd}/src/shared/api/sdk",
        "glob": "openapi.json",
        "additionalProperties": {
          "withInterfaces": true,
          "useOptions": true
        }
      }
    }
  }
}
